@model WagenparkMVC.Models.dealer

@{
	ViewBag.Title = "Delete";
	Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="center jumbotron">

	<h2 class="layoutSettings">Dealer verwijderen?</h2>

	<div class="layoutSettings">
		Dealer: @Html.DisplayFor(model => model.naam)
	</div>

	@using (Html.BeginForm())
	{
		@Html.AntiForgeryToken()

		<div class="form-actions no-color">
			<input type="submit" value="Delete" class="btn btn-warning" />
			<input value="Terug" class="btn btn-danger" onclick="location.href='@Url.Action("Index", "autoes")'" />
		</div>
	}
</div>
@section Scripts {
	@Scripts.Render("~/bundles/jqueryval")
	@Styles.Render("~/Content/main.css")
	@Styles.Render("~/Content/menu.css")
}
